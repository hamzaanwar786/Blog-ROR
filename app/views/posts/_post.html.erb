<tr id="<%#= dom_id post %>">
  <th scope="row"><%= post.id %></th>
  <td><%= post.title %></td>
  <td><%= post.body %></td>
  <td><%= post.views %></td>
  <td><%= link_to post.user.name, user_path(post.user) %></td>
  <td><%= post.created_at %></td>
  <td><%= link_to "View", post, class: "btn btn-success" %></td>
</tr>