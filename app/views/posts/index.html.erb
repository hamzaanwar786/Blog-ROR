<div class="container">
<p style="color: green"><%= notice %></p>

<h1>Posts</h1>

<div id="posts">
  <table class="table">
    <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Title</th>
      <th scope="col">Body</th>
      <th scope="col">CreateAt</th>
      <th scope="col">Actions</th>
    </tr>
    </thead>
    <tbody>
    <% @posts.each do |post| %>
      <%= render post %>
    <% end %>
    </tbody>
  </table>
</div>

<%= link_to "New post", new_post_path, class: "btn btn-primary" %>

</div>